<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    
    
   
    
     <ImageButton
     android:id="@+id/b7"
     android:src="@drawable/google"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
    />
    
     <ImageButton
     android:id="@+id/b8"
     android:src="@drawable/fb"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:layout_toRightOf="@+id/b7"
    />
     
     <ImageButton
     android:id="@+id/b9"
     android:src="@drawable/gmail"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:layout_toRightOf="@+id/b8"
    />
    
     <ImageButton
     android:id="@+id/b10"
     android:src="@drawable/linkedin"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:layout_below="@+id/b7"
    />
      
     <ImageButton
     android:id="@+id/b11"
     android:src="@drawable/twitter"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:layout_toRightOf="@+id/b10"
     android:layout_below="@+id/b8"
    />
     <ImageButton
     android:id="@+id/b12"
     android:src="@drawable/wikipedia"
     android:layout_height="wrap_content"
     android:layout_width="wrap_content"
     android:layout_toRightOf="@+id/b11"
     android:layout_below="@+id/b9"
    />

</RelativeLayout>
